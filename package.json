{
  "name": "pern-stacker",
  "version": "1.0.0",
  "description": "using psql, express, react, and node to create an app that will hit the Pokemon API and return a list of stats on a specific Pokemon",
  "main": "index.js",
  "scripts": {
    "dev": "run-p dev:**",
    "dev:server": "nodemon server.js",
    "dev:app": "cd client && yarn start",
    "build:app": "cd client && yarn build",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rubiksron/pern-stack.git"
  },
  "keywords": [
    "pern"
  ],
  "author": "ron dunphy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rubiksron/pern-stack/issues"
  },
  "homepage": "https://github.com/Rubiksron/pern-stack#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "create-react-app": "^3.4.1",
    "express": "^4.17.1",
    "nodemon": "^2.0.4",
    "pg": "^8.2.1"
  }
}